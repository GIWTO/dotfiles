        {
            "command": 
            {
                "action": "splitPane",
                "split": "right"
            },
            "keys": "alt+shift+plus"
        },
        {
            "command": 
            {
                "action": "copy"
            },
            "keys": "ctrl+shift+c"
        },
        {
            "command": "paste",
            "keys": "ctrl+shift+v"
        },
        {
            "command": 
            {
                "action": "splitPane",
                "split": "down"
            },
            "keys": "alt+shift+minus"
        },
        {
            "command": 
