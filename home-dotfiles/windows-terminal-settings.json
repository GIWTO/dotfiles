{
    "$help": "https://aka.ms/terminal-documentation",
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "actions": 
    [
        {
            "command": "closePane",
            "keys": "ctrl+shift+w"
        },
        {
            "command": 
            {
                "action": "copy"
            },
            "keys": "ctrl+shift+c"
        },
        {
            "command": 
            {
                "action": "splitPane",
                "split": "right"
            },
            "keys": "alt+shift+plus"
        },
        {
            "command": "paste",
            "keys": "ctrl+shift+v"
        },
        {
            "command": 
            {
                "action": "newTab"
            },
            "keys": "ctrl+shift+t"
        },
        {
            "command": 
            {
                "action": "splitPane",
                "split": "down"
            },
            "keys": "alt+shift+minus"
        },
        {
            "command": 
            {
                "action": "splitPane",
                "split": "auto"
            },
            "keys": "alt+shift+d"
        }
    ],
    "alwaysOnTop": false,
    "alwaysShowTabs": true,
    "copyOnSelect": false,
